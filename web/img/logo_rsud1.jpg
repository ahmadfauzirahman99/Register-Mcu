</head>
<body>

<img src="https://resizeimage.net/mypic/58n9mcWzz5wF2bbm/0HuW4/logo_rsud.jpg" border="0">
<script type="text/javascript">
function $get(id)
{
    return document.getElementById(id);
}
function SetImgPos( nWidthPic )
{
	var myElem = $get( "img_parent" );
	if( myElem != null )
		myElem.style.width = nWidthPic + "px";
}
function SwitchColor( strClolor )
{
	document.body.style.background = '#' + strClolor;
	var myElem = $get( "img_parent" );
	if( myElem != null )
		myElem.style.background = '#' + strClolor;
}
SetImgPos( 250 );
</script>
</body>
